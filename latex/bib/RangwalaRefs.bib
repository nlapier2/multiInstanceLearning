	Keywords = {metagenomics},
	Mesh = {Algorithms; Cluster Analysis; Genome; Genomics; Sequence Analysis, DNA},
	Pages = {316},
	Pmid = {19799776},
	Title = {Unsupervised statistical clustering of environmental shotgun sequences},
	Volume = {10},
	Year = {2009},
