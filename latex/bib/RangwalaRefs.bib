	Journal = {Appl Environ Microbiol},
	Journal-Full = {Applied and environmental microbiology},
	Keywords = {metagenomics},
	Mesh = {Computational Biology; Computer Simulation; Ecosystem; Gene Library; Genes, Viral; Genetic Variation; Genomics; Sequence Analysis, DNA; Sequence Homology},
