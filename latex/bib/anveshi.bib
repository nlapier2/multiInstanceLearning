	Mesh = {Algorithms; Amino Acid Motifs; Computational Biology; Databases, Protein; False Positive Reactions; Fungal Proteins; Models, Theoretical; Protein Binding; Protein Interaction Mapping; Protein Structure, Tertiary; ROC Curve; Software},
	Month = {Jun},
	Pages = {i38-46},
	Pmid = {15961482},
	Pst = {ppublish},
	Title = {Kernel methods for predicting protein-protein interactions},
	Volume = {21 Suppl 1},
